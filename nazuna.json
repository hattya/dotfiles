[
  {
    "name": "os",
    "layers": [
      {
        "name": "linux",
        "aliases": {
          "gocode": ".config/gocode",
          "peco": ".config/peco",
          "vimfiles": ".vim"
        },
        "subrepos": {
          ".local/Cellar": [
            {
              "src": "github.com/knu/z",
              "name": "knu-z"
            },
            {
              "src": "github.com/zsh-users/zsh-syntax-highlighting"
            }
          ]
        }
      },
      {
        "name": "windows",
        "aliases": {
          ".curlrc": "$APPDATA/_curlrc",
          "gocode": "$APPDATA/gocode",
          "peco": ".peco"
        }
      }
    ]
  },
  {
    "name": "master",
    "subrepos": {
      "vimfiles/bundle": [
        {
          "src": "github.com/junegunn/vim-plug"
        }
      ]
    }
  }
]
