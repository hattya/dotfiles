[alias]
	alias = "!f() { git config --get-regexp \"alias\\.${1:-*}\" | sed 's/^alias\\.\\([^ ]*\\) \\(.*\\)/\\1\\t= \\2/'; }; f | $(git var GIT_PAGER)"
	br = branch
	ci = commit -v
	co = checkout
	sw = checkout
	cp = cherry-pick
	cpc = cherry-pick --continue
	cpq = cherry-pick --quit
	cpa = cherry-pick --abort
	di = diff
	dic = diff --cached
	g = grep
	l = log --pretty=fuller --stat
	ll = log -p --pretty=fuller --stat
	ls = ls-files
	ss = status -bs
	st = status
	re = rebase --committer-date-is-author-date
	ri = rebase -i
	rc = rebase --continue
	rs = rebase --skip
	ra = rebase --abort
	v = show -p --pretty=fuller --stat
	view = log --decorate --source --tags --branches --oneline --graph
	xl = stash list
	xs = stash show
	xp = stash show -p --stat
	xd = stash drop
	xo = stash pop
	xa = stash apply
	xi = stash save
	xbr = stash branch
	xC = stash clear
[color]
	ui = auto
[core]
	pager = less -F
	quotepath = false
[diff]
	algorithm = histogram
	compactionHeuristic = true
	renames = copy
	tool = p4merge
[fetch]
	prune = true
[help]
	autocorrect = -1
[merge]
	tool = p4merge
[mergetool]
	keepBackup = false
[pager]
	diff = diff-highlight | $(git var GIT_PAGER)
	log = diff-highlight | $(git var GIT_PAGER)
	show = diff-highlight | $(git var GIT_PAGER)
[push]
	default = simple
[rebase]
	autosquash = true
[user]
	name = Akinori Hattori
	email = hattya@gmail.com
[github]
	user = hattya
