[defaults]
qnew = -D
tip = -v

[diff]
git = True

[ui]
username = Akinori Hattori <hattya@gmail.com>

[extensions]
hgext.bookmarks =
hgext.color =
hgext.mq =
hgext.pager =
hgext.progress =
hgext.purge =

diff_highlight =

[bookmarks]
track.current = True

[mq]
secret = True

[pager]
attend = annotate, bookmarks, branches, cat, config, diff, export, files, grep,
    heads, help, incoming, log, manifest, outgoing, paths, phase, status,
    summary, tags, tip,
    qapplied, qdiff, qseries, qunapplied
pager = less -F

%include .hgrc.d/os.rc
%include .hgrc.d/host.rc
