[alias]
	co = checkout
	ci = commit -v
	st = status
	ss = status -bs
	di = diff
	dic = diff --cached
	br = branch
	sw = checkout
	re = rebase --committer-date-is-author-date
	ri = rebase -i
	rc = rebase --continue
	rs = rebase --skip
	ra = rebase --abort
	ls = ls-files
	g = grep
	l = log --stat --pretty=fuller
	ll = log -p --stat --pretty=fuller
	v = show -p --pretty=fuller --stat
	view = log --oneline --graph --source --tags --branches
	xl = stash list
	xs = stash show
	xp = stash show -p --stat
	xd = stash drop
	xo = stash pop
	xa = stash apply
	xi = stash save
	xbr = stash branch
	xC = stash clear
[color]
	ui = auto
[core]
	quotepath = false
[diff]
	algorithm = histogram
	compactionHeuristic = true
	renames = copy
	tool = p4merge
[help]
	autocorrect = -1
[merge]
	tool = p4merge
[mergetool]
	keepBackup = false
[pager]
	diff = diff-highlight | less
	log = diff-highlight | less
	show = diff-highlight | less
[push]
	default = simple
[rebase]
	autosquash = true
[user]
	name = Akinori Hattori
	email = hattya@gmail.com
[github]
	user = hattya
